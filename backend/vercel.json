{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/models/(.*)",
      "dest": "src/models/$1",
      "methods": ["GET"]
    },
    {
      "src": "/routes/(.*)",
      "dest": "src/routes/$1",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
